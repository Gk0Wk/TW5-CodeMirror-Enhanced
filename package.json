        "@rollup/plugin-typescript": "^8.2.5",
        "clean-css-cli": "^5.4.1",
        "rollup": "^2.58.0",
        "terser": "^5.8.0",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3",
        "uglify-js": "^3.14.2"
    },
    "repository": {
