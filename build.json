{
    "plugin.info": {
        "version": "0.1.1",
        "type": "application/json",
        "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced",
        "plugin-type": "plugin",
        "parent-plugin": "$:/plugins/tiddlywiki/codemirror",
        "name": "CodeMirror TW5 Enhanced",
        "list": "readme ControlPanel",
        "description": "TiddlyWiki5 highlighting mode for CodeMirror",
        "dependents": "$:/plugins/tiddlywiki/codemirror-autocomplete",
        "author": "Gk0Wk",
        "email": "nmg_wk@yeah.net",
        "source": "https://github.com/Gk0Wk/TW5-CodeMirror-Enhanced"
    },
    "tiddlers": [{
            "src": "mode/tiddlywiki5.js",
            "fields": {
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/mode/tiddlywiki5.js",
                "module-type": "codemirror",
                "type": "application/javascript"
            }
        },
        {
            "src": "style.css",
            "fields": {
                "type": "text/css",
                "title": "$:/plugins/Gk0Wk/codemirror-tiddlywiki5/style.css",
                "tags": "$:/tags/Stylesheet"
            }
        },
        {
            "src": "mode/tex.js",
            "fields": {
                "type": "application/javascript",
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/mode/tex.js",
                "module-type": "codemirror"
            }
        },
        {
            "src": "readme.tw5",
            "fields": {
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/readme"
            }
        },
        {
            "src": "../dist/build/core.js",
            "fields": {
                "type": "application/javascript",
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/core.js",
                "module-type": "codemirror"
            }
        },
        {
            "src": "addon/hint/hint-tw5-tiddler.js",
            "fields": {
                "type": "application/javascript",
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/hint/hint-tw5-tiddler.js",
                "tags": "$:/CodeMirrorEnhanced/RealtimeHint",
                "module-type": "library"
            }
        },
        {
            "src": "addon/clickable/clickable-link.js",
            "fields": {
                "type": "application/javascript",
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/clickable/clickable-link.js",
                "tags": "$:/CodeMirrorEnhanced/ClickableToken",
                "module-type": "library"
            }
        },
        {
            "src": "ControlPanel.tw5",
            "fields": {
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/ControlPanel"
            }
        },
        {
            "text": "true",
            "fields": {
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/config/hint-preview"
            }
        },
        {
            "text": "true",
            "fields": {
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/config/clickable-link"
            }
        },
        {
            "text": "true",
            "fields": {
                "title": "$:/plugins/Gk0Wk/TW5-CodeMirror-Enhanced/config/realtime-hint"
            }
        }
    ]
}
