.cm-s-merbivore-soft{font-size:1em;line-height:1.5em;font-family:inconsolata,monospace;letter-spacing:.3px;word-spacing:1px;background:#1c1c1c;color:#e6e1dc}.cm-s-merbivore-soft .CodeMirror-lines{padding:8px 0}.cm-s-merbivore-soft .CodeMirror-gutters{box-shadow:1px 0 2px 0 rgba(0,0,0,.5);-webkit-box-shadow:1px 0 2px 0 rgba(0,0,0,.5);background-color:#1c1c1c;padding-right:10px;z-index:3;border:none}.cm-s-merbivore-soft div.CodeMirror-cursor{border-left:3px solid #e6e1dc}.cm-s-merbivore-soft .CodeMirror-activeline-background{background:#333435}.cm-s-merbivore-soft .CodeMirror-selected{background:#392243E0}.cm-s-merbivore-soft .cm-comment{font-style:italic;color:#ac4bb8}.cm-s-merbivore-soft .cm-number{color:null}.cm-s-merbivore-soft .cm-atom{color:null}.cm-s-merbivore-soft .cm-string{color:#8ec65f}.cm-s-merbivore-soft .cm-property{color:null}.cm-s-merbivore-soft .cm-keyword{color:null}.cm-s-merbivore-soft .cm-operator{color:null}.cm-s-merbivore-soft .CodeMirror-linenumber{color:italic}