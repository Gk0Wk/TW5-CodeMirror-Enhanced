.cm-s-mreq{font-size:1em;line-height:1.5em;font-family:inconsolata,monospace;letter-spacing:.3px;word-spacing:1px;background:#181818;color:#c4c4c4}.cm-s-mreq .CodeMirror-lines{padding:8px 0}.cm-s-mreq .CodeMirror-gutters{box-shadow:1px 0 2px 0 rgba(0,0,0,.5);-webkit-box-shadow:1px 0 2px 0 rgba(0,0,0,.5);background-color:#181818;padding-right:10px;z-index:3;border:none}.cm-s-mreq div.CodeMirror-cursor{border-left:3px solid #c4c4c4}.cm-s-mreq .CodeMirror-activeline-background{background:#020202}.cm-s-mreq .CodeMirror-selected{background:#c4c4c4}.cm-s-mreq .cm-comment{color:#6a6a6a}.cm-s-mreq .cm-string{color:#c59c70}.cm-s-mreq .cm-number{color:#6facbe}.cm-s-mreq .cm-atom{color:#6facbe}.cm-s-mreq .cm-keyword{color:#ca7473}.cm-s-mreq .cm-variable{color:#ca7473}.cm-s-mreq .cm-def{color:#c59c70}.cm-s-mreq .cm-variable-2{color:#ca7473}.cm-s-mreq .cm-property{color:#6facbe}.cm-s-mreq .cm-operator{color:#ca7473}.cm-s-mreq .CodeMirror-linenumber{color:#6a6a6a}