.cm-s-iplastic{font-size:1em;line-height:1.5em;font-family:inconsolata,monospace;letter-spacing:.3px;word-spacing:1px;background:#eeeEEEEB;color:#000}.cm-s-iplastic .CodeMirror-lines{padding:8px 0}.cm-s-iplastic .CodeMirror-gutters{box-shadow:1px 0 2px 0 rgba(0,0,0,.5);-webkit-box-shadow:1px 0 2px 0 rgba(0,0,0,.5);background-color:#eeeEEEEB;padding-right:10px;z-index:3;border:none}.cm-s-iplastic div.CodeMirror-cursor{border-left:3px solid #000}.cm-s-iplastic .CodeMirror-activeline-background{background:#0000001A}.cm-s-iplastic .CodeMirror-selected{background:#bad6fd}.cm-s-iplastic .cm-string{color:#093}.cm-s-iplastic .cm-number{color:null}.cm-s-iplastic .cm-keyword{color:#00f}.cm-s-iplastic .cm-atom{color:null}.cm-s-iplastic .cm-variable{color:#ff8000}.cm-s-iplastic .cm-def{font-style:italic}.cm-s-iplastic .cm-comment{font-style:italic;color:#06f}.cm-s-iplastic .cm-property{color:null}.cm-s-iplastic .cm-operator{color:#00f}.cm-s-iplastic .CodeMirror-linenumber{color:italic}