.cm-s-friendship-bracelet{font-size:1em;line-height:1.5em;font-family:inconsolata,monospace;letter-spacing:.3px;word-spacing:1px;background:#1f1f1f;color:#f1f1f1}.cm-s-friendship-bracelet .CodeMirror-lines{padding:8px 0}.cm-s-friendship-bracelet .CodeMirror-gutters{box-shadow:1px 0 2px 0 rgba(0,0,0,.5);-webkit-box-shadow:1px 0 2px 0 rgba(0,0,0,.5);background-color:#1f1f1f;padding-right:10px;z-index:3;border:none}.cm-s-friendship-bracelet div.CodeMirror-cursor{border-left:3px solid #f1f1f1}.cm-s-friendship-bracelet .CodeMirror-activeline-background{background:#404040}.cm-s-friendship-bracelet .CodeMirror-selected{background:#73597EE0}.cm-s-friendship-bracelet .cm-comment{color:#8a8988}.cm-s-friendship-bracelet .cm-keyword{color:#ff8c56}.cm-s-friendship-bracelet .cm-variable{color:#389ad9}.cm-s-friendship-bracelet .cm-def{font-style:italic}.cm-s-friendship-bracelet .cm-string{color:#f3f99a}.cm-s-friendship-bracelet .cm-variable-2{color:#fc8c56}.cm-s-friendship-bracelet .cm-property{color:#f66}.cm-s-friendship-bracelet .cm-atom{color:#f66}.cm-s-friendship-bracelet .cm-number{color:#f66}.cm-s-friendship-bracelet .cm-operator{color:#ff8c56}.cm-s-friendship-bracelet .CodeMirror-linenumber{color:#8a8988}