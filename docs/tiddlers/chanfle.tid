.cm-s-chanfle{font-size:1em;line-height:1.5em;font-family:inconsolata,monospace;letter-spacing:.3px;word-spacing:1px;background:#ffd513;color:#333}.cm-s-chanfle .CodeMirror-lines{padding:8px 0}.cm-s-chanfle .CodeMirror-gutters{box-shadow:1px 0 2px 0 rgba(0,0,0,.5);-webkit-box-shadow:1px 0 2px 0 rgba(0,0,0,.5);background-color:#ffd513;padding-right:10px;z-index:3;border:none}.cm-s-chanfle div.CodeMirror-cursor{border-left:3px solid #333}.cm-s-chanfle .CodeMirror-activeline-background{background:#333}.cm-s-chanfle .CodeMirror-selected{background:#fff}.cm-s-chanfle .cm-comment{color:#ddab14}.cm-s-chanfle .cm-string{color:#000}.cm-s-chanfle .cm-number{color:#888}.cm-s-chanfle .cm-atom{color:#888}.cm-s-chanfle .cm-keyword{color:red}.cm-s-chanfle .cm-variable{color:#888}.cm-s-chanfle .cm-def{font-style:italic;color:#fd971f}.cm-s-chanfle .cm-variable-2{color:red}.cm-s-chanfle .cm-property{color:#888}.cm-s-chanfle .cm-operator{color:red}.cm-s-chanfle .CodeMirror-linenumber{color:#ddab14}