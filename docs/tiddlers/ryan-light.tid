tags: $:/tags/Stylesheet
title: CodeMirrorThemes/ryan-light
type: text/css
module-type: codemirror-theme
name: ryan-light

.cm-s-ryan-light{font-size:1em;line-height:1.5em;font-family:inconsolata,monospace;letter-spacing:.3px;word-spacing:1px;background:#fff;color:#000}.cm-s-ryan-light .CodeMirror-lines{padding:8px 0}.cm-s-ryan-light .CodeMirror-gutters{box-shadow:1px 0 2px 0 rgba(0,0,0,.5);-webkit-box-shadow:1px 0 2px 0 rgba(0,0,0,.5);background-color:#fff;padding-right:10px;z-index:3;border:none}.cm-s-ryan-light div.CodeMirror-cursor{border-left:3px solid #000}.cm-s-ryan-light .CodeMirror-activeline-background{background:#efefef}.cm-s-ryan-light .CodeMirror-selected{background:#c3dcff}.cm-s-ryan-light .cm-comment{font-style:italic;color:grey}.cm-s-ryan-light .cm-number{color:#010e83}.cm-s-ryan-light .cm-atom{color:#010e83}.cm-s-ryan-light .cm-string{color:#0e8d90}.cm-s-ryan-light .cm-property{color:#010e83}.cm-s-ryan-light .cm-keyword{color:null}.cm-s-ryan-light .cm-operator{color:null}.cm-s-ryan-light .CodeMirror-linenumber{color:italic}